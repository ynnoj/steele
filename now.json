{
  "version": 2,
  "name": "steele.pro",
  "alias": "steele.pro",
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": {
        "distDir": "public"
      }
    }
  ],
  "build": {
    "env": {
      "GRAPHCMS_TOKEN": "steele-pro-graphcms-token",
      "GRAPHCMS_URL": "@steele-pro-graphcms-url"
    }
  }
}
